    "## This is our approach for a bar graph bloting the ratings of games \n",
    "\n",
    "plt.style.use('ggplot')\n",
    "\n",
    "x = ['The Witcher 3: Wild Hunt', 'Red Dead Redemption 2', 'Portal 2', 'Half-Life 2', 'Portal']\n",
    "Rating = [78.18, 71.15, 69.70, 62.92, 60.89]\n",
    "\n",
    "x_pos = [i for i, _ in enumerate(x)]\n",
    "\n",
    "plt.bar(x_pos, Rating, color='red')\n",
    "plt.xlabel(\"Game Titles\")\n",
    "plt.ylabel(\"Rating %\")\n",
    "plt.title(\"Highly Rated Games\")\n",
    "\n",
    "\n",
    "plt.xticks(x_pos, x,rotation =18)\n",
    "plt.tight_layout()\n",
    "plt.show()"
