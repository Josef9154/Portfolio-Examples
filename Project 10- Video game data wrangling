    "## Milestone 4 RapidAPI for Video Games"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [],
   "source": [
    "## Milestone 4 RapidAPI for Video Games\n",
    "# The next item is an API i plan to use from RapidAPI\n",
    "#You can see there are multiple keys that can be used here. \n",
    "#Names alone is the type of game, we have platform and lots of data. \n",
    "#This has 374K pieces of data. \n",
    "import pandas as pd\n",
    "import json\n",
    "import requests\n",
    "from pandas.io.json import json_normalize\n",
    "\n",
    "url = \"https://rawg-video-games-database.p.rapidapi.com/games\"\n",
    "headers = { \"x-rapidapi-host\": \"rawg-video-games-database.p.rapidapi.com\",\"x-rapidapi-key\": \"5ac7d80d6fmshf8a8b4da5bd3973p1ba163jsnc15e39a18bfb\"}\n",
    "Rapid_response = requests.request(\"GET\", url, headers=headers) \n",
    "\n",
    "# Print data returned (parsing as JSON)\n",
    "payload = Rapid_response.json()  # Parse `response.text` into JSON\n",
    "RapidStuff =json.dumps(payload, indent=4, sort_keys=True)\n",
    "RapidDict =json.loads(RapidStuff)\n",
    "\n",
    "#print(Rapid_response.text) ## I used pretty print but i think this looks better sometimes."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/html": [
       "<div>\n",
       "<style scoped>\n",
       "    .dataframe tbody tr th:only-of-type {\n",
       "        vertical-align: middle;\n",
       "    }\n",
       "\n",
       "    .dataframe tbody tr th {\n",
       "        vertical-align: top;\n",
       "    }\n",
       "\n",
       "    .dataframe thead th {\n",
       "        text-align: right;\n",
       "    }\n",
       "</style>\n",
       "<table border=\"1\" class=\"dataframe\">\n",
       "  <thead>\n",
       "    <tr style=\"text-align: right;\">\n",
       "      <th></th>\n",
       "      <th>0</th>\n",
       "    </tr>\n",
       "  </thead>\n",
       "  <tbody>\n",
       "    <tr>\n",
       "      <td>count</td>\n",
       "      <td>399021</td>\n",
       "    </tr>\n",
       "    <tr>\n",
       "      <td>next</td>\n",
       "      <td>https://api.rawg.io/api/games?page=2</td>\n",
       "    </tr>\n",
       "    <tr>\n",
       "      <td>previous</td>\n",
       "      <td>None</td>\n",
