    "## THis represents 3 graphs in style of bar, scatter, and plot to that can be a best practices of selection\n",
    "\n",
    "plt.style.use('ggplot')\n",
    "data = {'Fallout 3': 476, 'Disgaea': 453, 'Monster Hunter Freedom': 445, 'Animal Crossing: Wild World': 437,'Animal Crossing: City Folk': 431, }\n",
    "names = list(data.keys())\n",
    "values = list(data.values())\n",
    "\n",
    "fig, axs = plt.subplots(1, 3, figsize=(9, 3), sharey=True)\n",
    "axs[0].bar(names, values)\n",
    "axs[1].scatter(names, values)\n",
    "axs[2].plot(names, values)\n",
    "fig.suptitle('Longest Played Games')\n"
   ]
