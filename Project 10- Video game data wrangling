   ],
   "source": [
    "# I found a Youtube trick that was nice on column names\n",
    "rows = table.find_all('tr') #Change this to 'tr' or 'td'\n",
    "columns = [v.text.replace('\\n','').replace('[a]','') for v in rows[0].find_all('th')]\n",
    "print (columns)"
   ]
  },
